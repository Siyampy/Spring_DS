<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
			"https://www.springframework.org/dtd/spring-beans-2.0.dtd">
			<beans>
<!-- But here we can actually use the engine as the inner bean andso it need not be used separately -->
<bean id="c" class="beans.Car">
<property name="name">
<list>
<value>Audi</value>
<value>BMW</value>
<value>Rolls Royce</value>

</list>
</property>
<property name="engine">
<list>
<bean id="e1" class="beans.Engine">
<property name="model"><value>A50</value></property>

</bean>
<bean id="e2" class="beans.Engine">
<property name="model"><value>BMW 3</value></property>

</bean>
<bean id="e3" class="beans.Engine">
<property name="model"><value>Rolls 500</value></property>

</bean>

</list>

</property>

</bean>
</beans>